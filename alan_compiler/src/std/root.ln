  let line = groupname.join(' ');
  "\u{1b}[33;1m ".stdout;
  line.stdout;
  "\u{1b}[0m".stdout;
  let res = comparator(actual, expected);
  if(res,
    fn { "\u{0d}\u{1b}[32;1m".stdout; },
    fn { "\u{0d}\u{1b}[31;1m".stdout; });
  line.stdout;
  "\u{1b}[0m".print;
  if(!res, fn {
    "\u{1b}[31;1mTest Failure\u{1b}[0m".eprint;
    Maybe{i64}()!!;
  }, fn {
    "".stdout;
  }); // TODO: Better way to force an exit */
  return groupname;
}

