
        from @std/app import start, print, exit

        prefix toInt16 as ~ precedence 10

        on start {
          print(~1 & ~2);
          print(~1 | ~3);
          print(~5 ^ ~3);
          print(! ~0);
          print(~1 !& ~2);
          print(~1 !| ~2);
          print(~5 !^ ~3);
          emit exit 0;
        }
      
