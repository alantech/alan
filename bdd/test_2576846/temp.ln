        on start {
          if true {
            print(1);
            if 1 == 2 {
              print('What?');
            } else {
              print(2);
              if 2 == 1 {
                print('Uhh...');
              } else if 2 == 2 {
                print(3);
              } else {
                print('Nope');
              }
            }
          } else {
            print('Hmm');
          }
          emit exit 0;
        }
      
